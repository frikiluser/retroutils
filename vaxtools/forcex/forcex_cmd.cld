MODULE FORCEX_CMD
IDENT "V01.01"

DEFINE VERB FORCEX
	IMAGE "FORCEX"
	QUALIFIER IDENTIFICATION, LABEL=PID, SYNTAX=FORCEX_PID
	QUALIFIER KILL, NEGATABLE 
	PARAMETER P1,VALUE(REQUIRED), PROMPT="Process name",LABEL=PROCNAME

DEFINE SYNTAX FORCEX_PID
	IMAGE "FORCEX"
	QUALIFIER KILL, NEGATABLE 
	PARAMETER P1,VALUE(REQUIRED), PROMPT="Pid", LABEL=PID


